{
  "name": "TvShows",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:e2e": "vite --mode e2e",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "e2e:local": "concurrently --kill-others \"npm run dev:e2e\" \"npm run cypress:open\""
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@vue/test-utils": "^2.4.1",
    "core-js": "^3.29.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.0",
    "roboto-fontface": "*",
    "vue": "^3.2.0",
    "vue-i18n": "^9.4.1",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@pinia/testing": "^0.1.3",
    "@types/lodash": "^4.14.198",
    "@types/node": "^18.15.0",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "cypress": "^13.2.0",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^22.1.0",
    "sass": "^1.60.0",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.2.0"
  }
}
